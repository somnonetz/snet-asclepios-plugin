		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="xnat:imageSessionData"/>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="encPsgScanData">
		<xs:annotation>
			<xs:documentation>Polysomnographic Scan</xs:documentation>
		</xs:annotation>
